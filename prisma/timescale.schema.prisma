generator client {
  provider = "prisma-client-js"
  output   = "./generated/timescale-client"
}

datasource db {
  provider = "postgresql"
  url      = env("TIMESCALE_DATABASE_URL")
}

model BiosensorData {
  id                String   @id @default(uuid()) 
  vid               String
  config            String
  frequency         Int
  bioImpedance      Float    // Changed to Float
  phaseAngle        Float    // Changed to Float
 time               DateTime   @db.Timestamp(3) 
  


  @@index([config])
}

model TemperatureData {
  id                 String   @id  @default(uuid()) 
  vid                String
  config            String
  frequency         Int
  temperature        Float   // Changed to Float
  time            DateTime     @db.Timestamp(3) 

 
  @@index([config])
}

model GsrData {
  id          String   @id  @default(uuid()) 
  vid         String
  config      String
  frequency   Int
  gsr         Float    // Changed to Float
  time        DateTime   @db.Timestamp(3) 
  
  
  @@index([config])
}

model GlucoseData {
  id             String   @id   @default(uuid()) 
  vid            String
  config         String
  frequency      Int
  glucose        Float   // Changed to Float
  time           DateTime   @db.Timestamp(3) 
  
  
 
  @@index([config])
}
